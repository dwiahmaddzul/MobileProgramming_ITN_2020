<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Herbarium1818101">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sequlite">


        <activity android:name="com.example.Herbarium1818101.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.Herbarium1818101.MainCreate"></activity>
        <activity android:name="com.example.Herbarium1818101.MainUpdel"></activity>
        <activity android:name="com.example.Herbarium1818101.MainRead"></activity>
    </application>

</manifest>